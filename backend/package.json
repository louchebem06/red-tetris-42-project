{
	"name": "backend",
	"version": "1.0.0",
	"description": "",
	"main": "app.js",
	"scripts": {
		"test": "npm run env && jest --detectOpenHandles --no-cache --forceExit > src/tests/output.txt && npm run env-rm",
		"testDev": "jest --coverage --detectOpenHandles --watchAll --no-cache",
		"check": "open src/tests/coverage-reports/lcov-report/index.html",
		"build": "npx tsc --build",
		"start": "npm run env && npm run build && TZ='UTC' node dist/app.js",
		"dev": "TZ='UTC' nodemon src/app.ts",
		"lint": "prettier . --check . && eslint .",
		"lint:fix": "prettier --check . && eslint . --fix",
		"format": "prettier --write .",
		"indent": "npm run format && npm run lint",
		"ibt": "npm run indent && npm run build && npm run test",
		"wsdoc": "open http://localhost:8080/ws-docs",
		"env": "npx ts-node configSetup.ts",
		"env-rm": "rm -rf .env"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@babel/preset-env": "^7.22.15",
		"@babel/preset-typescript": "^7.22.15",
		"@types/express": "^4.17.17",
		"@types/jest": "^29.5.5",
		"@types/node": "^20.5.9",
		"@types/socket.io": "^3.0.2",
		"@types/supertest": "^2.0.12",
		"@types/uuid": "^9.0.6",
		"@typescript-eslint/eslint-plugin": "^6.5.0",
		"@typescript-eslint/parser": "^6.5.0",
		"eslint": "^8.48.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-prettier": "^5.0.0",
		"jest": "^29.6.4",
		"nodemon": "^3.0.1",
		"prettier": "^3.0.3",
		"socket.io": "^4.7.2",
		"socket.io-client": "^4.7.2",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.1",
		"typescript": "^5.2.2"
	},
	"dependencies": {
		"@asyncapi/cli": "^0.60.1",
		"@asyncapi/generator": "^1.12.0",
		"@asyncapi/parser": "^2.1.0",
		"body-parser": "^1.20.2",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"supertest": "^6.3.3",
		"uuid": "^9.0.1"
	}
}
